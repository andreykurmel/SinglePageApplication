{
    "name": "loshmis/vanguard",
    "description": "Advanced PHP Login and User Management",
    "keywords": ["advanced", "secure", "login", "user management", "authentication", "authorization", "register"],
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/loshmis/laravel-countries"
        },
        {
            "type": "git",
            "url": "https://github.com/andreykurmel/tablda_data_receiver"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "andreykurmel/tablda_data_receiver": "dev-master",
        "anhskohbo/no-captcha": "3.6.0",
        "anlutro/l4-settings": "v1.4.1",
        "doctrine/dbal": "3.10.0",
        "dompdf/dompdf": "v3.1.0",
        "endroid/qr-code": "6.0.3",
        "google/apiclient": "v2.18.2",
        "intervention/image-laravel": "1.5.6",
        "jenssegers/agent": "v2.6.4",
        "laracasts/presenter": "0.2.7",
        "laravel/framework": "v9.52.20",
        "laravel/socialite": "v5.21.0",
        "laravelcollective/html": "v6.4.1",
        "league/fractal": "0.19.1",
        "maatwebsite/excel": "3.1.62",
        "paragonie/random_compat": "v2.0.21",
        "paypal/paypal-checkout-sdk": "1.0.2",
        "paypal/rest-api-sdk-php": "1.14.0",
        "phpoffice/phpword": "1.3.0",
        "predis/predis": "v2.2.0",
        "proengsoft/laravel-jsvalidation": "v4.9.3",
        "sendgrid/sendgrid": "8.1.9",
        "stripe/stripe-php": "v16.4.0",
        "symfony/expression-language": "v7.1.0",
        "twilio/sdk": "8.3.9",
        "tymon/jwt-auth": "2.1.1",
        "webpatser/laravel-countries": "1.5.2"
    },
    "require-dev": {
        "symfony/css-selector": "v6.4.13",
        "symfony/dom-crawler": "v6.4.23",
        "mockery/mockery": "1.6.12",
        "phpunit/phpunit": "9.6.23",
        "barryvdh/laravel-ide-helper": "v2.15.1",
        "barryvdh/laravel-debugbar": "v3.14.9",
        "laravel/browser-kit-testing": "v6.4.0",
        "filp/whoops": "2.17.0"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "Vanguard\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],

        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
